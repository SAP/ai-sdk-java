{
  "config": {
    "modules": {
      "prompt_templating": {
        "model": {
          "name": "gpt-4o",
          "params": {
            "presence_penalty": 0,
            "frequency_penalty": 0,
            "max_tokens": 50,
            "temperature": 0.1,
            "top_p": 1,
            "n": 1
          },
          "version": "latest",
          "timeout" : 600,
          "max_retries" : 2
        },
        "prompt": {
          "template": [
            {
              "role": "user",
              "content": "Hello World! Why is this phrase so famous?"
            }
          ],
          "defaults": {},
          "tools": []
        }
      },
      "masking": {
        "providers": [
          {
            "type": "sap_data_privacy_integration",
            "method": "pseudonymization",
            "entities": [
              {
                "type": "profile-phone"
              }
            ],
            "allowlist": [],
            "mask_grounding_input": {
              "enabled": false
            }
          }
        ]
      }
    }
  },
  "placeholder_values": {},
  "messages_history": []
}
