{
  "config": {
    "modules": {
      "prompt_templating": {
        "model": {
          "name": "gpt-4o",
          "params": {},
          "version": "latest",
          "timeout" : 600,
          "max_retries" : 2
        },
        "prompt": {
          "template": [
            {
              "role": "user",
              "content": "What is the weather in Potsdam and in Toulouse?"
            }
          ],
          "defaults": {},
          "tools": [
            {
              "type": "function",
              "function": {
                "description": "Get the weather in location",
                "name": "getCurrentWeather",
                "parameters": {
                  "$schema": "https://json-schema.org/draft/2020-12/schema",
                  "additionalProperties": false,
                  "type": "object",
                  "properties": {
                    "arg0": {
                      "type": "object",
                      "properties": {
                        "location": {
                          "type": "string"
                        },
                        "unit": {
                          "type": "string",
                          "enum": [
                            "C",
                            "F"
                          ]
                        }
                      },
                      "required": [
                        "location",
                        "unit"
                      ]
                    }
                  },
                  "required": [
                    "arg0"
                  ]
                },
                "strict": false
              }
            }
          ]
        }
      }
    }
  },
  "placeholder_values": {},
  "messages_history": []
}
